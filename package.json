{
  "name": "ez-press",
  "version": "1.0.1",
  "description": "CLI tool to generate a simple express app with typescript with clean architecture and the common required middlewares and packages",
  "main": "ezpress.js",
  "bin": {
    "ezpress": "dist/ezpress.js"
  },
  "repository": {
    "url": "https://github.com/Ahmed1monm/EzPress.git",
    "type": "git"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "start": "node dist/ezpress.js",
    "prestart": "npm run build",
    "test": "jest",
    "lint": "eslint --fix . --ext .ts"
  },
  "keywords": [
    "Ez-press",
    "CLI",
    "express",
    "automation",
    "typescript",
    "clean-architecture",
    "middlewares",
    "packages",
    "nodejs",
    "backend",
    "api",
    "restful-api",
    "restful",
    "restful-api-generator",
    "restful-api-automation",
    "restful-api-automation-tool",
    "restful-api-automation-cli",
    "restful-api-automation-cli-tool",
    "restful-api-automation-cli-tool-generator",
    "graph",
    "scaffold",
    "template"
  ],
  "author": "Ahmed Mone'm",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
